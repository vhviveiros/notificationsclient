{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Hermes application",
            "request": "attach",
            "type": "reactnativedirect",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Debug Android",
            "type": "reactnative",
            "request": "launch",
            "platform": "android",
            "sourceMaps": true,
            "outDir": "${workspaceFolder}/.vscode/.react",
            "enableDebug": true
        }
    ]
}
